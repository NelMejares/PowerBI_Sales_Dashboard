        "partitions": [
          {
            "name": "Calendar",
            "mode": "import",
            "source": {
              "expression": [
                "",
                "VAR BaseCalendar = CALENDARAUTO()",
                "RETURN",
                "    ADDCOLUMNS(",
                "        BaseCalendar,",
                "        \"Year\", YEAR([Date]),",
                "        \"Quarter\", QUARTER([Date]),",
                "        \"Month No\", MONTH([Date]),",
                "        \"Week Num\", WEEKNUM([Date]),",
                "        \"Week Day\", WEEKDAY([Date]),",
                "        \"Day\", DAY([Date]),",
                "        \"Month Name\", FORMAT([Date], \"MMMM\"),",
                "        \"Month N\", FORMAT([Date], \"MMM\"),",
                "        \"Weekday Name\", FORMAT([Date], \"ddd\"),",
                "        \"Quater  Number\", \"Q\" & QUARTER([Date]),",
                "        \"Year Month\", FORMAT([Date], \"YYYY MMMM\"),",
                "        \"Start of Week\", [Date] + (2 - WEEKDAY([Date])),",
                "        \"End of Week\", [Date] + (7 - WEEKDAY([Date])),",
                "        \"Weekday Weekend\",",
                "            IF(",
                "                    WEEKDAY([Date]) IN {1, 7}, \"Weekend\", \"Weekday\"",
                "            )",
                "",
                "    )"
              ],
              "type": "calculated"
            }
          }
        ]
      },
